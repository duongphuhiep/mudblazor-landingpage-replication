@page "/tryserver"
@rendermode InteractiveServer

@inject IJSRuntime JSRuntime;
@inject ILogger<Tryserver> _logger;

@code {
    public bool JSRuntimeInProcess => JSRuntime is IJSInProcessRuntime;
    public bool BrowserOS => OperatingSystem.IsBrowser();

    protected override void OnInitialized()
    {
        _logger.LogInformation($"{nameof(OnInitialized)}, JSRuntimeInProcess={JSRuntimeInProcess}, BrowserOS={BrowserOS}");
    }

    protected override void OnAfterRender(bool firstRender)
    {
        _logger.LogInformation($"{nameof(OnAfterRender)}, JSRuntimeInProcess={JSRuntimeInProcess}, BrowserOS={BrowserOS}, firstRender={firstRender}");
    }

}